  Spina.Scaffold = (function() {
    function Scaffold() {}

    Scaffold.init = function() {
      return $(document).enhance();
    };

    Scaffold.enhance = function(el) {
      var $el;
      $el = $(el);
      return $el.find('.infinite-pagination').infiniteScroll();
    };

    return Scaffold;

  })();

  $.fn.enhance = function() {
    return Spina.Scaffold.enhance(this);
  };

  $(document).on('page:change', function() {
    return Spina.Scaffold.init();
  });

}).call(this);
